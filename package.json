{
  "name": "search-e2e-test",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "npm run test:clean-reports && npm run test:headless && npm run test:merge-reports",
    "test:clean-reports": "rm -rf ./results",
    "test:headed": "npx cypress run --headed",
    "test:headless": "set -e NODE_ENV=dev && ./start-tests.sh",
    "test:merge-json": "npx mochawesome-merge /results/json/*.json -o /results/json/mochawesome-merge-results.json",
    "test:merge-reports": "npm run test:merge-xml && npm run test:merge-json",
    "test:merge-xml": "npx junit-merge --dir=/results -o /results/merge-test-results.xml"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:open-cluster-management/search-e2e-test.git"
  },
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "engines": {
    "node": "10.15.3",
    "npm": "6.9.0"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "del": "^4.1.1",
    "log4js": "^4.2.0",
    "nconf": "^0.10.0",
    "nconf-yaml": "^1.0.2",
    "xml2json": "^0.11.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.7.2",
    "chromedriver": "^83.0.0",
    "cypress": "^4.10.0",
    "cypress-multi-reporters": "^1.4.0",
    "cypress-terminal-report": "^1.4.1",
    "eslint-plugin-cypress": "^2.11.1",
    "geckodriver": "^1.19.1",
    "junit-report-merger": "0.0.6",
    "mocha": "^8.1.0",
    "mocha-junit-reporter": "^2.0.0",
    "mochawesome": "^6.1.1",
    "mochawesome-merge": "^4.1.0",
    "querystring": "^0.2.0",
    "selenium-standalone": "^6.16.0"
  }
}
