-include /opt/build-harness/Makefile.prow

.PHONY: install
install:
	npm config set prefix '/tmp'
	export PATH=/tmp:$PATH
	npm install

.PHONY: lint
lint:
	sudo npm run lint

.PHONY: check
check:
	sudo npm run check

.PHONY: unit-test
unit-test:
	sudo npm run test

.PHONY: test-e2e
test-e2e:
	start-tests.sh
